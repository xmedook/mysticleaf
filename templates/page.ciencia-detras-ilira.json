    "custom_liquid_6amBKF": {
      "type": "custom-liquid",
      "custom_css": [
        "section .section-header {max-width: 1200px !important;}",
        ".estudios-grid {padding: 40px 0px;}",
        "h2 {font-weight: 700 !important; text-align: center; padding-bottom: 25px;}",
