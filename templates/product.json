        "custom_liquid": "{%- style -%}\n@import url('https://fonts.googleapis.com/css2?family=Manrope:wght@200..800&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=DM+Sans:ital,opsz,wght@0,9..40,100..1000;1,9..40,100..1000&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=Gentium+Book+Plus:ital,wght@0,400;0,700;1,400;1,700&display=swap');\n\n\n.ph-section-container {\n      font-family: 'DM Sans', sans-serif; \n      line-height: 1.5;\n      background-color: #fff; \n          background-image: url(https://cdn.shopify.com/s/files/1/0940/2155/0363/files/Fondo_21_estudios.png?v=1747242051);\n    padding: 123px;\n\n    }\n\n    .ph-main-banner-wrapper {\n      padding: 20px 0; \n    }\n\n    .ph-main-banner {\n      background-color: #1E4A8B; \n      color: white !important;\n      padding: 35px 40px; \n      border-radius: 150px 0px 150px 0px;\n      width: 95%;\n      max-width: 1100px; \n      margin: 0 auto; \n      position: relative;\n      box-shadow: 0 8px 25px rgba(0,0,0,0.15);\n    }\n\n    .ph-title-section {\n      text-align: center;\n      margin-bottom: 35px;\n    }\n    .ph-title-section h1 {\n      font-size: clamp(2.8em, 5vw, 4em); \n      font-weight: 900; \n      margin: 0;\n      line-height: 1;\n      text-transform: uppercase;\n    color: white !important;\n    }\n    .ph-title-section h2 {\n      font-size: clamp(1.8em, 3.5vw, 2.5em);\n      font-family: \"Gentium Book Plus\";\n      font-weight: 500; \n      margin: 8px 0 0 0;\n      color: #BDD4F1 !important;\n      text-transform: uppercase;\n    }\n\n    .ph-product-details-flex {\n      display: flex;\n      align-items: flex-start; \n      justify-content: space-between;\n      gap: 30px; \n      margin-bottom: 40px;\n    }\n\n    .ph-claims-column {\n      flex: 1 1 220px; \n      display: flex;\n      flex-direction: column;\n      gap: 30px; /* Space between individual claim blocks */\n    }\n\n    .ph-claim h3 {\n      font-size: clamp(1.2em, 2.5vw, 1.6em);\n      font-weight: 700; /* Bold */\n      margin: 0 0 8px 0;\n      color: white !important;\n    }\n    .ph-claim p {\n      color: white !important;\n      font-size: clamp(0.9em, 2vw, 1.05em);\n      margin: 0;\n      line-height: 1.45;\n      color: #E0EFFF; \n    }\n\n    .ph-product-image-container {\n      flex: 1 1 300px; \n      text-align: center;\n      align-self: center; \n      padding: 0 15px; \n    }\n    .ph-product-image-container img {\n      max-width: 100%;\n      width: 320px; \n      height: auto;\n      filter: drop-shadow(8px 8px 15px rgba(0,0,0,0.25)); \n    }\n\n    .ph-footer-section {\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      margin-top: 30px;\n      gap: 20px;\n      flex-wrap: wrap; \n    }\n\n    .ph-logos {\n      display: flex;\n      gap: 20px;\n      align-items: center;\n      flex-wrap: wrap; \n    }\n    .ph-logos img {\n      height: 85px; \n      width: auto;\n      \n      padding: 3px;\n      border-radius: 5px;\n    }\n\n    .ph-content-info {\n      background-color: white;\n      color: #1E4A8B; /* Dark blue text to match banner */\n      padding: 12px 25px;\n      border-radius: 25px; /* Rounded corners */\n      font-weight: bold;\n      font-size: clamp(1em, 2.2vw, 1.25em);\n      white-space: nowrap; /* Keep \"Contenido: 210 g\" on one line */\n      box-shadow: 0 2px 5px rgba(0,0,0,0.1);\n    }\n\n    /* Responsive adjustments */\n    @media (max-width: 992px) { /* Tablet landscape and smaller */\n      .ph-main-banner {\n        padding: 30px 30px;\n      }\n      .ph-product-details-flex {\n        gap: 20px; /* Reduce gap */\n      }\n      .ph-claims-column {\n        flex-basis: 180px; /* Allow columns to be a bit narrower */\n      }\n      .ph-product-image-container {\n        flex-basis: 280px; /* Adjust image container basis */\n      }\n       .ph-product-image-container img {\n        width: 280px; /* Reduce image size */\n      }\n 